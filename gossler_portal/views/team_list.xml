<odoo>
    <data>
        <template id="team_list">

          <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-6">
            <h6>Equipo <t t-esc="request.env.user.department_id.name"/></h6>
            <div class="graybar">
              <t t-foreach="request.env.user.department_id.member_ids" t-as="line">
                <t t-if="request.env.user.employee_ids.parent_id.parent_id.parent_id">
                <div class="table1">
                      <div class="row1 fixed">
                          <img class="rounded-circle d-block  organigram-photo" t-attf-src="data:image/*;base64,{{line.image_128}}"/>
                      </div>
                  <div class="row1 fluid">
                       <div clow="row">
                        <div class="col-12 text-small"><strong><t t-esc="line.name"/></strong></div>
                        <t t-if="line.job_id.name">
                          <div class="col-12 text-smallest"><t t-esc="line.job_id.name"/></div>
                        </t>
                      </div>
                  </div>
                </div>
                </t>
              </t>
            </div>
          </div>

        </template>
    </data>
</odoo>
